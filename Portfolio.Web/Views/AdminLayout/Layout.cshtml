
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">


<!-- [Head] start -->
@await Component.InvokeAsync("_AdminLayoutHeadComponent")
<!-- [Head] end -->

<!-- [Body] Start -->

<body data-pc-preset="preset-1" data-pc-direction="ltr" data-pc-theme="light">
  <!-- [ Pre-loader ] start -->
<div class="loader-bg">
  <div class="loader-track">
    <div class="loader-fill"></div>
  </div>
</div>
<!-- [ Pre-loader ] End -->

 <!-- [ Sidebar Menu ] start -->
    @await Component.InvokeAsync("_AdminLayoutSideBarComponent")
<!-- [ Sidebar Menu ] end --> 



<!-- [ Header Topbar ] start -->
    @await Component.InvokeAsync("_AdminLayoutHeaderComponent")
<!-- [ Header ] end -->





  <!-- [ Main Content ] start -->
  <div class="pc-container">
    <div class="pc-content">
      <!-- [ breadcrumb ] start -->
      <div class="page-header">
        <div class="page-block">
          <div class="row align-items-center">
            <div class="col-md-12">
              <div class="page-header-title">
                <h3 class="m-b-10">@ViewData["Title"]</h3>
              </div>
              <br />
              <ul class="breadcrumb">
                <li class="breadcrumb-item"><a href="#">Ana Sayfa</a></li>
                <li class="breadcrumb-item"><a href="#">@ViewData["Title"]</a></li>
                
              </ul>
            </div>
          </div>
        </div>
      </div>
      <!-- [ breadcrumb ] end -->
      <!-- [ Main Content ] start -->
     @RenderBody()
    </div>
  </div>
  <!-- [ Main Content ] end -->


    <!-- [ Footer ] -->
    @await Component.InvokeAsync("_AdminLayoutFooterComponent")
    <!-- [ Footer ] -->



    <!-- [ Script ] -->
    @await Component.InvokeAsync("_AdminLayoutScriptComponent")
    <!-- [ Script ] -->


    

</body>
<!-- [Body] end -->

</html>